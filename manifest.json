{
  "manifest_version": 3,
  "name": "Rem-px converter Extension",
  "version": "1.0.0",
  "description": "Convert px to rems and rems to px",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_icon": {
      "16": "icon.png",
      "24": "icon.png",
      "32": "icon.png"
    },
    "default_title": "Rem-px converter Extension title",
    "default_popup": "popup.html"
  },
  "permissions": ["storage"]
}
